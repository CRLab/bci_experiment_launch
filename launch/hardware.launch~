<launch>
  <param name="bhd/canbus_number" type="int" value="32" />
  
  <!-- Type of hand installed. Should be one of:
         * "280" For a BH280 Hand (CANbus communication)
         * "280+TACT" For a BH280 Hand with the fingertip tactile array
       -->
  <param name="bhd/hand_type" type="string" value="280+TACT" />

  <param name="bhd/hand_publish_frequency" type="int" value="40"/>
 
  <param name="bhd/log_canbus_data" type="bool" value="false" />

 <node pkg="owd" type="canbhd" output="screen" respawn="false" cwd="node" name="bhd" launch-prefix="/usr/bin/roxterm --profile=hardware --separate --title=hardware --tab-name=owd -e"/>


 <node name="staubli_controller" pkg="staubliTX60"  type="staubliTX60_server" args="http://192.168.11.254:5653/" launch-prefix="/usr/bin/roxterm --profile=hardware --tab --tab-name=staubli -e"/>
 
 <node name="calibration_launcher" pkg="bci_experiment_launch" type="launch_scripts.bash" args=" calibration launchKinectRobot.launch" launch-prefix="/usr/bin/roxterm --profile=hardware --tab --tab-name=calibration -e" />



</launch>